# Ignore Maven's target directory (contains compiled classes and build artifacts)
target/
# Ignore Maven wrapper jar if using binary type (uncommon, script type is default)
.mvn/wrapper/maven-wrapper.jar
# Ignore build directories in source folders (if any sub-projects use them)
**/src/main/**/target/
**/src/test/**/target/

# Ignore Maven wrapper properties backup files (if generated by wrapper plugin during updates)
.mvn/wrapper/maven-wrapper.properties.backup

# --- IntelliJ IDEA ---
# Ignore IntelliJ project files
.idea/modules.xml
.idea/jarRepositories.xml
.idea/compiler.xml
.idea/libraries/
*.iws
*.iml
*.ipr

# --- Eclipse ---
# Ignore Eclipse project files and build artifacts
.apt_generated
.classpath
.factorypath
.project
.settings
.springBeans
.sts4-cache

# --- NetBeans ---
# Ignore NetBeans build and project files
/nbproject/private/
/nbbuild/
/dist/
/nbdist/
/.nb-gradle/
/build/
# But *don't* ignore build directories inside source code (if they exist for some reason)
!**/src/main/**/build/
!**/src/test/**/build/

# --- VS Code ---
# Ignore VS Code settings directory
.vscode/

# --- Mac OS ---
# Ignore Mac-specific metadata files
.DS_Store

# --- Maven Wrapper (Lite/Script Type - Default) ---
# If using the 'only-script' type (default), these might be generated temporarily and should be ignored
.mvn/wrapper/MavenWrapperDownloader.java

# --- Maven Release Plugin ---
# Ignore files generated by the Maven Release Plugin
pom.xml.tag
pom.xml.releaseBackup
pom.xml.versionsBackup
pom.xml.next
release.properties
dependency-reduced-pom.xml
buildNumber.properties
.mvn/timing.properties

# --- Eclipse m2e (Maven Integration for Eclipse) ---
# Ignore core Eclipse project file
.project
# Ignore JDT (Java Development Tools) settings file
.classpath